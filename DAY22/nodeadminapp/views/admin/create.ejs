            <!--우측 콘텐츠 영역 -->
            <div class="wrapper wrapper-content">
                <form action="/admin/create" method="post">
                    <div class="ibox-content m-b-sm border-bottom">


                        <div class="row">
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">부서명</label>
                                    <select name="dept_name" class="form-control">
                                        <option value="0">전체</option>
                                        <option value="1">연구개발</option>
                                        <option value="2">인사팀</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">아이디</label>
                                    <input type="text" name="admin_id" class="form-control">
                                </div>
                            </div>
    
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">비밀번호</label>
                                    <input type="password" name="admin_password" class="form-control">
                                </div>
                            </div>
    
                        </div>
    
    
                        <div class="row">
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">메일주소</label>
                                    <input type="email" name="email" class="form-control">
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">이름</label>
                                    <input type="text" name="admin_name" class="form-control">
                                </div>
                            </div>
    
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">전화번호</label>
                                    <input type="text" name="telephone" class="form-control">
                                </div>
                            </div>
    
                        </div>
    
    
                        <div class="row">
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">사용여부</label>
                                    <select name="used_yn_code" class="form-control">
                                        <option value="0">사용중</option>
                                        <option value="1">일시중지</option>
                                        <option value="2">탈퇴상태</option>
                                    </select>
                                </div>
                            </div>
    
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <label class="control-label">등록일시</label>
                                    <input type="text" name="reg_date" class="form-control" value="2023-12-19" readonly>
                                </div>
                            </div>
    
                        </div>
    
    
                        <div class="text-center">
                            <button type="submit" class="btn btn-primary">저장</button>
                            <a href="/admin/list" class="btn btn-info">목록</a>
                        </div>
    
                    </div>
                </form>

            </div>

